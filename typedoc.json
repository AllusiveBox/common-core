{
  "categorizeByGroup": true,
  "cleanOutputDir": true,
  "defaultCategory": "Other",
  "entryPoints": [
    "./.src/"
  ],
  "entryPointStrategy": "resolve",
  "excludePrivate": true,
  "hideGenerator": true,
  "includeVersion": true,
  "includes": "./",
  "out": "docs",
  "readme": "ChangeLog.md",
  "logLevel": "Verbose",
  "markedOptions": {
    "breaks": false,
    "langPrefix": "typescript"
  },
  "sort": ["static-first","visibility", "alphabetical"],
  "validation": {
    "invalidLink": true,
    "notExported": true
  }
}
